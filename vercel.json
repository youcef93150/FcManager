{
  "name": "psg-frontend",
  "builds": [
    {
      "src": "VueFront/package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist",
        "buildCommand": "cd VueFront && npm ci && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/VueFront/dist/$1"
    }
  ]
}
